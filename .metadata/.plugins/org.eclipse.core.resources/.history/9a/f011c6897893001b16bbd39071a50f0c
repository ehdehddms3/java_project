package com.ict03.class07;

enum Type2 {
	// 순서를 지키자.
	// 1. 들어갈 정보
	WALK("워킹화", 270),
	RUN("러닝화",275),
	TRACK("트래킹화", 265),
	HIK("하이킹화",260);
	// 2. 전역변수
	final private String name;
	final private int size;
	
	// 3. 생성자
	Type2(String name, int size){
		this.name = name;
		this.size = size;
	}
	
}
public class Ex03 {

}
